- const contacts = LANG.contacts;

section.container#contacts
    .contacts
        h2.header= contacts.header
        .contacts__block
            h4.contacts__block-header= contacts.company_name
            p.contacts__block-text= contacts.address
            p.contacts__block-text= contacts.company_number
            a(class="link" href=contacts.email_path)= contacts.email
            a(class="link" href=contacts.phone_path)= contacts.phone